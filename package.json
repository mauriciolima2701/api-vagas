{
    "name": "api-vagas",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
        "build": "rimraf dist && tsc",
        "ts.check": "tsc --project tsconfig.json",
        "add-build": "git add dist",
        "typeorm": "typeorm-ts-node-commonjs",
        "entity:create": "npm run typeorm entity:create",
        "migration:create": "npm run typeorm migration:create ./src/app/shared/migrations/CreateTable",
        "migration:generate": "npm run typeorm migration:generate -- -d ./src/main/config/typeorm.config.ts",
        "migration:run": "npm run typeorm migration:run -- -d ./src/main/config/typeorm.config.ts",
        "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/main/config/typeorm.config.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^18.14.6",
        "@types/pg": "^8.6.6",
        "@types/reflect-metadata": "^0.1.0",
        "@types/uuid": "^9.0.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "ioredis": "^5.3.1",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.10",
        "uuid": "^9.0.0"
    },
    "pre-commit": [
        "ts.check",
        "build",
        "add-build"
    ]
}
